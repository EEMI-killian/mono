{% extends 'base.html.twig' %}

{% block title %}Outfit{% endblock %}

{% block body %}
    <h1>Outfit</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ outfit.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ outfit.name }}</td>
            </tr>
            <tr>
                <th>ImageUrl</th>
                <td>{{ outfit.imageUrl }}</td>
            </tr>
            <tr>
                <th>AddAt</th>
                <td>{{ outfit.addAt ? outfit.addAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
            <th>PromptResult</th>
            <td>{{ outfit.promptResult }}</td>
            </tr>

        </tbody>
    </table>

    <a href="{{ path('app_outfit_index') }}">back to list</a>

    <a href="{{ path('app_outfit_edit', {'id': outfit.id}) }}">edit</a>

    {{ include('outfit/_delete_form.html.twig') }}
{% endblock %}
